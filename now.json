{
  "version": 2,
  "public": true,
  "alias": [
    "xp-graphql-relay-modern.now.sh"
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/main.js",
      "dest": "/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}